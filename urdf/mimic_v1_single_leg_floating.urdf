<?xml version="1.0" ?>
<robot name="mimic_v1_single_leg_floating">
  
  <!-- World link (固定点) -->
  <link name="world"/>
  
  <!-- Floating base joint (6DOF: 3D position + 3D orientation) -->
  <joint name="floating_base" type="floating">
    <parent link="world"/>
    <child link="base_link"/>
    <origin xyz="0 0 0.5" rpy="0 0 0"/>
  </joint>
  
  <!-- Base link -->
  <link name="base_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="../meshes/base_link.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="../meshes/base_link.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0.000919 0.000893 -0.000839" rpy="0 0 0"/>
      <mass value="4.277848"/>
      <inertia ixx="0.03065" ixy="0.0009113" ixz="0.00157" iyy="0.01819" iyz="5.5322119e-05" izz="0.03977"/>
    </inertial>
  </link>
  
  <!-- Upper link R -->
  <link name="upper_link_R">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="../meshes/upper_link_R.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="../meshes/upper_link_R.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0.0 -0.001797 -0.087228" rpy="0 0 0"/>
      <mass value="0.072264"/>
      <inertia ixx="0.0002834" ixy="-1.205e-17" ixz="1.15e-16" iyy="0.0002835" iyz="1.3568424999999999e-05" izz="8.566684e-06"/>
    </inertial>
  </link>
  
  <joint name="upper_link_R_joint" type="continuous">
    <parent link="base_link"/>
    <child link="upper_link_R"/>
    <origin xyz="0.0 -0.1325 0.0" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
  </joint>
  
  <!-- Lower link R -->
  <link name="lower_link_R">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="../meshes/lower_link_R.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="../meshes/lower_link_R.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0.0 -0.001797 -0.08" rpy="0 0 0"/>
      <mass value="0.072264"/>
      <inertia ixx="0.0002581" ixy="-1.0659999999999998e-17" ixz="1.0520000000000001e-16" iyy="0.0002582" iyz="1.221312e-05" izz="8.566684e-06"/>
    </inertial>
  </link>
  
  <joint name="lower_link_R_joint" type="continuous">
    <parent link="upper_link_R"/>
    <child link="lower_link_R"/>
    <origin xyz="0.0 0.0 -0.175" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
  </joint>
  
  <!-- Wheel R -->
  <link name="wheel_R">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="../meshes/wheel_R.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="../meshes/wheel_R.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="-0.0 0.0 0.0" rpy="0 0 0"/>
      <mass value="0.058969"/>
      <inertia ixx="5.011064125e-05" ixy="0.0" ixz="0.0" iyy="9.176784999999999e-05" iyz="0.0" izz="5.011064125e-05"/>
    </inertial>
  </link>
  
  <joint name="wheel_R_joint" type="continuous">
    <parent link="lower_link_R"/>
    <child link="wheel_R"/>
    <origin xyz="0.0 0.0 -0.16" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
  </joint>
  
</robot>